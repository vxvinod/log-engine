<div id='container'>
	<h1>Log Engine</h1>
	<%= form_for @log, url: :Logpage_index do |f| %>
		Path: <%= f.text_field :path, :value => @path||nil %>
		<%= f.submit :ready %>
	<% end %>
	<br>	
	Regular Expression : <%= text_field_tag "regular_expression" %>
	<div id="display_content"><%=@display%></div>
</div>